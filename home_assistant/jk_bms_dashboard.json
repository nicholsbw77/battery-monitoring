{
  "dashboard": {
    "title": "JK BMS Monitor",
    "uid": "jk-bms-monitor",
    "tags": ["battery"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "5s"
  },
  "overwrite": true
}
    "panels": [
      {
        "id": 1,
        "title": "State of Charge",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [{
          "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery\")\n  |> filter(fn: (r) => r[\"_field\"] == \"soc\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 20, "color": "orange"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Battery Voltage",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [{
          "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery\")\n  |> filter(fn: (r) => r[\"_field\"] == \"voltage\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "volt",
            "decimals": 2,
            "color": {"mode": "value"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 48, "color": "orange"},
                {"value": 51, "color": "green"},
                {"value": 56, "color": "orange"}
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "none"
        }
      },
      {
        "id": 3,
        "title": "Current",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [{
          "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery\")\n  |> filter(fn: (r) => r[\"_field\"] == \"current\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "amp",
            "decimals": 2,
            "color": {"mode": "value"},
            "thresholds": {
              "steps": [
                {"value": -100, "color": "blue"},
                {"value": -0.1, "color": "green"},
                {"value": 0.1, "color": "orange"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "none"
        }
      },
      {
        "id": 4,
        "title": "Power",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [{
          "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery\")\n  |> filter(fn: (r) => r[\"_field\"] == \"power\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "watt",
            "decimals": 1
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "none"
        }
      },
      {
        "id": 5,
        "title": "Cell Statistics",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 6, "y": 4},
        "targets": [
          {
            "refId": "A",
            "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery_cells\")\n  |> filter(fn: (r) => r[\"_field\"] == \"cell_max\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
          },
          {
            "refId": "B",
            "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery_cells\")\n  |> filter(fn: (r) => r[\"_field\"] == \"cell_min\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
          },
          {
            "refId": "C",
            "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery_cells\")\n  |> filter(fn: (r) => r[\"_field\"] == \"cell_diff\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "volt",
            "decimals": 3
          }
        },
        "options": {
          "textMode": "value_and_name",
          "graphMode": "none"
        }
      },
      {
        "id": 6,
        "title": "Individual Cell Voltages",
        "type": "table",
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 8},
        "targets": [{
          "query": "from(bucket: \"battery_data\")\n  |> range(start: -30s)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery_cell_detail\")\n  |> filter(fn: (r) => r[\"_field\"] == \"voltage\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> last()\n  |> group(columns: [\"cell_number\"])\n  |> sort(columns: [\"cell_number\"])"
        }],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background-solid"
            },
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 3.0, "color": "orange"},
                {"value": 3.2, "color": "green"},
                {"value": 3.5, "color": "yellow"},
                {"value": 3.65, "color": "red"}
              ]
            }
          }
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "cell_number": "Cell",
                "_value": "Voltage (V)"
              },
              "excludeByName": {
                "_time": true,
                "_start": true,
                "_stop": true,
                "_measurement": true,
                "_field": true,
                "battery_id": true,
                "type": true
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "cellHeight": "sm"
        }
      },
      {
        "id": 7,
        "title": "Cell Voltages (Time Series)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20},
        "targets": [{
          "query": "from(bucket: \"battery_data\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery_cell_detail\")\n  |> filter(fn: (r) => r[\"_field\"] == \"voltage\")\n  |> filter(fn: (r) => r[\"battery_id\"] == \"jk_bms_1\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)"
        }],
        "fieldConfig": {
          "defaults": {
            "unit": "volt",
            "decimals": 3,
            "custom": {
              "drawStyle": "line",
              "lineWidth": 1,
              "fillOpacity": 10
            }
          }
        }
      }
    ]
  }
}